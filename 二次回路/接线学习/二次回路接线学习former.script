'''''''''''''''''''''
animation{
	name{空动画}
	controller{
		life{0,2000}
		
	}
}

''''''''''''''''''''重置所有动画'''''''''''''''''
resetanimation(aniname){
	animation{
		name{重置动画}
		controller{
			action{
				act{
					type{reset animation}				
					animation{name{aniname}}
				}
			}
		}
	}	

}

animation{''重置连线动画
	name{重置连线动画}
	resetanimation(IO盒供电N_stretch)
	resetanimation(IO盒供电P_stretch)
	resetanimation(IPC供电N_stretch)
	resetanimation(IPC供电P_stretch)
	resetanimation(电源开关-接触器_stretch)
	resetanimation(电源指示灯接线_stretch)
	resetanimation(电源指示灯接线2_stretch)
	resetanimation(端子排N-示教器_stretch)
	resetanimation(端子排P-示教器_stretch)
	resetanimation(端子排-电源开关_stretch)
	resetanimation(接触器-端子排N_stretch)
	resetanimation(电源开关-电源指示灯_stretch)
}


'''''''''''''''''''''模型闪烁函数 hightfield（）'''''''''''''''''''''

hilightfield(field1,field2){''闪烁
	animation{
		controller{
			life{0,400}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{TRUE}
				}
			}		
		}
		controller{
			life{400,800}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{false}
				}
			}		
		}
		controller{
			life{800,1200}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{true}
				}
			}		
		}
		controller{
			life{1200,1600}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{false}
				}
			}		
		}
		controller{
			life{1600,2000}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{true}
				}
			}		
		}
		controller{
			life{2000,2400}
			action{
				Act{
				type{hilight field}
				field{field1}
				BOOL{false}
				}
			}		
		}
		controller{
			life{2400,2800}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{TRUE}
				}
			}		
		}
		controller{
			life{2800,3200}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{false}
				}
			}		
		}
		controller{
			life{3200,3600}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{true}
				}
			}		
		}
		controller{
			life{3600,4000}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{false}
				}
			}		
		}
		controller{
			life{4000,4400}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{true}
				}
			}		
		}
		controller{
			life{4400,4800}
			action{
				Act{
				type{hilight field}
				field{field2}
				BOOL{false}
				}
			}		
		}
	}
}


'''''''''''''''''''''每一段的接线动画''''''''''''''''''''''''''''''''
int linenum=1;
setlinenum(num){''设置连线
	animation{
		controller{
			action{
				program{
					linenum=num;
				}
			}
		}
	}
}

animation{''电源开关-接触器接线动画
	name{启动电源开关-接触器}
	setlinenum(0)
	hilightfield(/scene/model/控制面板/电源开关,/scene/model/X3端子排/对象378)
	animation{name{端子排-电源开关_stretch}}
	hilightfield(/scene/model/控制面板/电源开关,/scene/model/交流接触器/jcq00)
	animation{name{电源开关-接触器_stretch}}
	hilightfield(/scene/model/控制面板/电源开关,/scene/model/交流接触器/jcq00)
	animation{name{接触器-端子排N_stretch}}
	setlinenum(2)
}

animation{''电源指示灯接线
	name{电源指示灯接线}
