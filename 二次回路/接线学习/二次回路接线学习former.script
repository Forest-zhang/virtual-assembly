	animation{name{电源开关-电源指示灯_stretch}}
	hilightfield(/scene/model/控制面板/电源指示灯,/scene/model/X3端子排/对象374)
	animation{name{电源指示灯接线2_stretch}}
	setlinenum(3)
}

animation{''IO盒供电接线
	name{IO盒供电接线}
	setlinenum(0)
	hilightfield(/scene/model/IO模块/hcnc_2-DC_Shell,/scene/model/X3端子排/对象378)
