key_map{
	name{空格键}
	key{ctrl}
	enable{false}
	group{2111}
	reaction{
		type{key down}
		action{
			program{
				if(linenum==1){
					astart("启动电源开关-接触器");
					astart("交流接触器接线讲解");
				}
				else if(linenum==2){
					astart("电源指示灯接线");
					astart("电源指示灯接线讲解")
				}
				else if(linenum==3){
					astart("IO盒供电接线");
					astart("IO盒供电接线");						
				}
				else if(linenum==4){
					astart("IPC供电接线");
					astart("IPC供电接线讲解");					
				}			
			}		
		}
	}
}

